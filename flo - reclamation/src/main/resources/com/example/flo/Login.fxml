<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="500.0" stylesheets="@./MainStyle.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.flo.controllers.LoginController" style="-fx-background-color: white">

    <Button layoutX="100.0" layoutY="20" mnemonicParsing="false" onAction="#front"
            text="Front" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>

    <Button mnemonicParsing="false" onAction="#back" text="Back"
            layoutY="80" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>
</AnchorPane>
