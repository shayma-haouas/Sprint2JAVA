<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.Details"
            prefHeight="400.0" prefWidth="600.0">

    <AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <Label layoutX="31.0" layoutY="28.0" text="Event Name:" />
            <Label layoutX="31.0" layoutY="65.0" text="Event Type:" />
            <Label layoutX="31.0" layoutY="101.0" text="Event Location:" />
            <Label fx:id="nameLabel" layoutX="150.0" layoutY="28.0" />
            <Label fx:id="typeLabel" layoutX="150.0" layoutY="65.0" />
            <Label fx:id="placeLabel" layoutX="150.0" layoutY="101.0" />
            <Button fx:id="participateButton" layoutX="150.0" layoutY="150.0" mnemonicParsing="false" onAction="#participateInEvent" text="Participate" />
        </children>
    </AnchorPane>







</AnchorPane>
